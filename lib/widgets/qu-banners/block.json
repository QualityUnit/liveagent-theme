{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "qu/statistics",
	"version": "0.1.0",
	"title": "Statistics",
	"icon": "chart-bar",
	"category": "widgets",
	"textdomain": "statistics",
	"editorScript": "file:./build/index.js",
	"attributes": {
		"text": {
			"type": "string",
			"source": "text",
			"selector": "Statistics--block--text",
			"default": "The average set net… – enter your message"
		},
		"value": {
			"type": "string",
			"source": "text",
			"selector": "Statistics--block--value",
			"default": "74%"
		},
		"source": {
			"type": "string",
			"source": "text",
			"selector": "Statistics--block--source",
			"default": "Source"
		},
		"style": {
			"type": "string",
			"default": "Statistics--block__negative has-bg"
		},
		"color": {
			"type": "string",
			"default": "1"
		}
	}
}
